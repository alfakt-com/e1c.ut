Процедура ДобавитьДанныеПоСотрудникам(Буфер, 
									СтруктураДокумента,
									КоллекцияСвойств,
									ВыгружитьДанныеПоГЦВП, 
									ВидДанных,
									ЗаполнятьПоказательТипа)
	
		Для каждого ВыгружаемыйРеквизит Из КоллекцияСвойств Цикл
			
			Значение = ПривестиЗначение(ВыгружаемыйРеквизит.Значение);
			
			Если НЕ ПустаяСтрока(Значение) Тогда
				Если Не ЗаполнятьПоказательТипа И ВыгружаемыйРеквизит.Ключ = "ПоказательТипа" Тогда
					ДобавитьВСтроку(Буфер, ВыгружаемыйРеквизит.Ключ + "="); // Секция должна выгружаться пустой, потому что в банках может остаться ПО, которое ожидает такой ключ.
				Иначе
					ДобавитьВСтроку(Буфер, ВыгружаемыйРеквизит.Ключ + "=" + Значение);
				КонецЕсли;
			КонецЕсли;
			
		КонецЦикла;
		
		ДобавитьВСтроку(Буфер, "СПИСОКСОТРУДНИКОВ");
		
		Если ВидДанных <> "ЗП" И ВыгружитьДанныеПоГЦВП Тогда
			
			Если Не ПустаяСтрока(КоллекцияСвойств.ПолучательБИН_ИИН) Тогда
				ДобавитьВСтроку(Буфер, "ГЦВПБИН_ИИН="+КоллекцияСвойств.ПолучательБИН_ИИН);
			КонецЕсли;
			Если Не ПустаяСтрока(КоллекцияСвойств.ПолучательИИК) Тогда
				ДобавитьВСтроку(Буфер, "ГЦВПИИК="+КоллекцияСвойств.ПолучательИИК);
			КонецЕсли;
			Если Не ПустаяСтрока(КоллекцияСвойств.ПолучательБанкНаименование) Тогда
				ДобавитьВСтроку(Буфер, "ГЦВПБанкНаименование="+КоллекцияСвойств.ПолучательБанкНаименование);
			КонецЕсли;
			Если Не ПустаяСтрока(КоллекцияСвойств.ПолучательБанкБИК) Тогда
				ДобавитьВСтроку(Буфер, "ГЦВПБанкБик="+КоллекцияСвойств.ПолучательБанкБИК);
			КонецЕсли;
			
		КонецЕсли;	
		
		Запрос = Новый Запрос;
		
		Если ВидДанных = "ЗП" Тогда
			ТекстЗапроса = СформироватьТекстЗапросаПоЗП();
		Иначе
			ТекстЗапроса = СформироватьТекстЗапросаПоОтчислениям();
		КонецЕсли;
		
		Запрос.Текст = ТекстЗапроса;	
		Запрос.УстановитьПараметр("Документ",СтруктураДокумента.Ссылка); 
		
		ОбщаяСумма = 0;
		
		ВыборкаСотрудников = Запрос.Выполнить().Выбрать();
		
		Пока ВыборкаСотрудников.Следующий() Цикл
			Если НЕ ПустаяСтрока(ВыборкаСотрудников.Сотрудник) Тогда
				
				Если ВидДанных = "ЗП" Тогда
					Если ПустаяСтрока(ВыборкаСотрудников.СотрудникКартСчет) Тогда
						
						ТекстСообщения  = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
						НСтр("ru = 'Не указан номер карт-счета сотрудника: %1 %2 %3'"),
						СокрЛП(ВыборкаСотрудников.Фамилия),
						СокрЛП(ВыборкаСотрудников.Имя), 
						СокрЛП(ВыборкаСотрудников.Отчество));
						
						ОбщегоНазначенияКлиентСервер.СообщитьПользователю(ТекстСообщения);

					КонецЕсли;
				КонецЕсли;

				Если ВидДанных = "ЗП" Тогда
					ДобавитьВСтроку(Буфер, "СОТРУДНИК="+СокрЛП(ВыборкаСотрудников.Фамилия) + " " + СокрЛП(ВыборкаСотрудников.Имя) + " " + СокрЛП(ВыборкаСотрудников.Отчество));
				Иначе	
					ДобавитьВСтроку(Буфер, "СОТРУДНИК="+ВыборкаСотрудников.Сотрудник);
				КонецЕсли;
				
				Если Не ВидДанных = "ЗП" Тогда
					Если НЕ ПустаяСтрока(ВыборкаСотрудников.РегистрацияФамилия) Тогда 
						ДобавитьВСтроку(Буфер, "Регистрация="+СокрЛП(ВыборкаСотрудников.РегистрацияФамилия)+" "+СокрЛП(ВыборкаСотрудников.РегистрацияИмя)+" "+СокрЛП(ВыборкаСотрудников.РегистрацияОтчество));
					Иначе
						ДобавитьВСтроку(Буфер, "Регистрация="+ВыборкаСотрудников.Сотрудник);
					КонецЕсли;
				КонецЕсли;	
				
				Если НЕ ПустаяСтрока(ВыборкаСотрудников.СотрудникБИН_ИИН) Тогда
					ДобавитьВСтроку(Буфер, "СотрудникБИН_ИИН="+ВыборкаСотрудников.СотрудникБИН_ИИН);
				КонецЕсли;
				
				Если ВидДанных = "ЗП" Тогда
					ДобавитьВСтроку(Буфер, "СотрудникНомерКартСчета="+ВыборкаСотрудников.СотрудникКартСчет);
					
				Иначе
					
					Если НЕ ВыборкаСотрудников.СотрудникДатаРождения = Дата(1,1,1) Тогда
						ДобавитьВСтроку(Буфер, "СотрудникДатаРождения=" + Формат(ВыборкаСотрудников.СотрудникДатаРождения, "ДФ=dd.MM.yyyy"));
					Иначе 
						ТекстСообщения  = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
						НСтр("ru = 'Не заполнена дата рождения сотрудника %1'"),
						ВыборкаСотрудников.Сотрудник);

						ОбщегоНазначенияКлиентСервер.СообщитьПользователю(ТекстСообщения);
					КонецЕсли;
					
					Если НЕ ВыборкаСотрудников.Период = Дата(1,1,1) Тогда
						ДобавитьВСтроку(Буфер, "Период=" + НРег(Формат(ВыборкаСотрудников.Период, "ДФ='ММгггг'")));
					КонецЕсли;
				КонецЕсли;	
					
				Если НЕ ПустаяСтрока(ВыборкаСотрудников.Сумма) Тогда
					ДобавитьВСтроку(Буфер, "Сумма="+Формат(ВыборкаСотрудников.Сумма, "ЧРД=.; ЧРГ=' '; ЧГ=0; ЧЦ=15; ЧДЦ=2"));
					ОбщаяСумма = ОбщаяСумма + ВыборкаСотрудников.Сумма;
				Иначе
					Если ВидДанных = "ЗП" Тогда
						ТекстСообщения  = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
						НСтр("ru = 'Не заполнена сумма заработной платы сотрудника: %1'"),
						ВыборкаСотрудников.Сотрудник);
					Иначе	
						ТекстВид = ?(ВидДанных = "ОПВ", "пенсионных перечислений", "социальных отчислений");
						
						ТекстСообщения = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(
											НСтр("ru = 'Не заполнена сумма %1 сотрудника: %2 за %3 г.'"),
												ТекстВид,
												ВыборкаСотрудников.Сотрудник,
												НРег(Формат(ВыборкаСотрудников.Период, "ДФ='ММММ гггг'")));
										
					КонецЕсли;
					
					ОбщегоНазначенияКлиентСервер.СообщитьПользователю(ТекстСообщения);

				КонецЕсли;
				
				ДобавитьВСтроку(Буфер, "КОНЕЦСОТРУДНИК");

			КонецЕсли;
		КонецЦикла;
		
		ДобавитьВСтроку(Буфер, "КОНЕЦСПИСКАСОТРУДНИКОВ");

		Если ВидДанных = "ЗП" Тогда
			Если ОбщаяСумма <> СтруктураДокумента.Сумма Тогда
				СуммаКомиссии = СтруктураДокумента.Сумма - ОбщаяСумма;
				
				ДобавитьВСтроку(Буфер, "СуммаКомиссииБанка=" + Формат(СуммаКомиссии, "ЧРД=.; ЧРГ=' '; ЧГ=0; ЧЦ=15; ЧДЦ=2"));
				
			КонецЕсли;
			
		КонецЕсли;	
	
КонецПроцедуры	

Функция СекцияДокумент(СтруктураДокумента, ЗаполнятьПоказательТипа, ВидыДокументов)
	
	Буфер = "";
	Реквизит = "";
	
	Секция = СтруктураЭкспорта();
	ЗаполнитьЗначениеЭкспорта(Секция, СтруктураДокумента);
	
	ДобавитьВСтроку(Буфер, "СекцияДокумент=" + ВидыДокументов[СтруктураДокумента.ВидДокумента]);
	
	ПенсионныеОтчисления_Есть 	 	= ПривестиЗначение(СтруктураДокумента["ПенсионныеОтчисления"]);
	СоциальныеОтчисления_Есть	 	= ПривестиЗначение(СтруктураДокумента["СоциальныеОтчисления"]);
	ПеречислениеНаКартСчета_Есть 	= ПривестиЗначение(СтруктураДокумента["ПеречислениеНаКартСчета"]);
	
	Если ПеречислениеНаКартСчета_Есть = "1" Тогда 	
		
		ДобавитьДанныеПоСотрудникам(Буфер, СтруктураДокумента, Секция, Ложь, "ЗП", ЗаполнятьПоказательТипа);
		
	ИначеЕсли ПенсионныеОтчисления_Есть = "1" Тогда 	
		
		ДобавитьДанныеПоСотрудникам(Буфер, СтруктураДокумента, Секция, Истина, "ОПВ", ЗаполнятьПоказательТипа);
		
	ИначеЕсли СоциальныеОтчисления_Есть = "1" Тогда 	
		
		ДобавитьДанныеПоСотрудникам(Буфер, СтруктураДокумента, Секция, Истина, "СО", ЗаполнятьПоказательТипа);
		
	Иначе	
		Для каждого ВыгружаемыйРеквизит Из Секция Цикл
			
			Значение = ПривестиЗначение(ВыгружаемыйРеквизит.Значение);
			
			Если НЕ ПустаяСтрока(Значение) Тогда
				Если Не ЗаполнятьПоказательТипа И ВыгружаемыйРеквизит.Ключ = "ПоказательТипа" Тогда
					ДобавитьВСтроку(Буфер, ВыгружаемыйРеквизит.Ключ + "="); // Секция должна выгружаться пустой, потому что в банках может остаться ПО, которое ожидает такой ключ.
				Иначе
					ДобавитьВСтроку(Буфер, ВыгружаемыйРеквизит.Ключ + "=" + Значение);
				КонецЕсли;
			КонецЕсли;
			
		КонецЦикла;
	КонецЕсли;
	
	ДобавитьВСтроку(Буфер, "КонецДокумента");
	
	Возврат Буфер;
	
КонецФункции

Процедура ЗаполнитьЗначениеЭкспорта(СтрокаЭкспорта, ВыборкаДляЭкспорта)
	
	Если ВыборкаДляЭкспорта.ВидДокумента = Перечисления.ТипыПлатежныхДокументов.ПлатежноеТребование Тогда
		Плательщик = "Контрагент";
		Получатель = "Организация";
	Иначе
		Плательщик = "Организация";
		Получатель = "Контрагент";
	КонецЕсли;
	
	СтрокаЭкспорта.НомерДокумента = ПрефиксацияОбъектовКлиентСервер.НомерНаПечать(
		ВыборкаДляЭкспорта.Номер,
		Истина, // УдалитьПрефиксИнформационнойБазы
		Истина); // УдалитьПользовательскийПрефикс
	СтрокаЭкспорта.ДатаДокумента  = Формат(ВыборкаДляЭкспорта.Дата, "ДФ=дд.ММ.гггг");
	СтрокаЭкспорта.Сумма          = Формат(ВыборкаДляЭкспорта.Сумма, "ЧЦ=15; ЧДЦ=2; ЧРД=.; ЧРГ=' '; ЧГ=0");
	
// ПеречислениеНаКартСчета, ПенсионныеОтчисления, СоциальныеОтчисления
	СтрокаЭкспорта.ПеречислениеНаКартСчета 	= ВыборкаДляЭкспорта["ПеречислениеНаКартСчета"];
	СтрокаЭкспорта.ПенсионныеОтчисления 	= ВыборкаДляЭкспорта["ПенсионныеОтчисления"];
	СтрокаЭкспорта.СоциальныеОтчисления 	= ВыборкаДляЭкспорта["СоциальныеОтчисления"];
	
	СтрокаЭкспорта.ПлательщикИИК  = ВыборкаДляЭкспорта[Плательщик + "РасчСчет"];
	СтрокаЭкспорта.ПолучательИИК  = ВыборкаДляЭкспорта[Получатель + "РасчСчет"];
	
	НепрямыеРасчетыУПлательщика   = ЗначениеЗаполнено(ВыборкаДляЭкспорта[Плательщик + "БанкДляРасчетов"]);
	НепрямыеРасчетыУПолучателя    = ЗначениеЗаполнено(ВыборкаДляЭкспорта[Получатель + "БанкДляРасчетов"]);
	
// Код - идентификатор платежа
	СтрокаЭкспорта.Код = ВыборкаДляЭкспорта.Код;
	
// Плательщик, ПлательщикБИН_ИИН, ПлательщикКБЕ
	СтрокаЭкспорта.ПлательщикНаименование = ВыборкаДляЭкспорта[Плательщик + "Наим"];
	
	Значение = ВыборкаДляЭкспорта[Плательщик + "ИНН"];
	СтрокаЭкспорта.ПлательщикБИН_ИИН = Значение;
	СтрокаЭкспорта.ПлательщикКБЕ     = ВыборкаДляЭкспорта[Плательщик + "КБЕ"];
	
// ПлательщикСчет, ПлательщикБанкНаименование, ПлательщикГородБанка, ПлательщикБИКБанка, ПлательщикБанкИИК
//
	СтрокаЭкспорта.ПлательщикБанкНаименование = ВыборкаДляЭкспорта[Плательщик + ?(НепрямыеРасчетыУПолучателя, "БанкДляРасчетов", "Банк")];
	СтрокаЭкспорта.ПлательщикБанкБИК 	= ВыборкаДляЭкспорта[Плательщик + ?(НепрямыеРасчетыУПолучателя, "БИКРЦБанка",      "БИКБанка")];
	СтрокаЭкспорта.ПлательщикБанкИИК 	= ВыборкаДляЭкспорта[Плательщик + ?(НепрямыеРасчетыУПолучателя, "КоррСчетРЦБанка", "КоррСчет")];
	
// ПолучательНаименование
	ПолучательПозицияСимволВК = СтрНайти(ВыборкаДляЭкспорта[Получатель + "Наим"], Символы.ВК);
	Если ПолучательПозицияСимволВК = 0 Тогда
		СтрокаЭкспорта.ПолучательНаименование = ВыборкаДляЭкспорта[Получатель + "Наим"];
	КонецЕсли;
	
//// Получатель, ПолучательИНН
//	СтрокаЭкспорта.ПолучательИНН = ВыборкаДляЭкспорта[Получатель + "ИНН"];
//	СтрокаЭкспорта.Получатель = "ИНН " + ВыборкаДляЭкспорта[Получатель + "ИНН"] + " " + СтрокаЭкспорта.Получатель1;
	
// Получатель, ПолучательБИН_ИИН, ПолучательКБЕ
	СтрокаЭкспорта.ПолучательБИН_ИИН = ВыборкаДляЭкспорта[Получатель + "ИНН"];
	СтрокаЭкспорта.ПолучательКБЕ     = ВыборкаДляЭкспорта[Получатель + "КБЕ"];
	
// ПолучательСчет, ПолучательБанкНаименование, ПолучательГородБанка, ПолучательБИКБанка, ПолучательБанкИИК
//
	СтрокаЭкспорта.ПолучательБанкНаименование = ВыборкаДляЭкспорта[Получатель + ?(НепрямыеРасчетыУПолучателя, "БанкДляРасчетов", "Банк")];
	СтрокаЭкспорта.ПолучательБанкБИК          = ВыборкаДляЭкспорта[Получатель + ?(НепрямыеРасчетыУПолучателя, "БИКРЦБанка",      "БИКБанка")];
	СтрокаЭкспорта.ПолучательБанкИИК          = ВыборкаДляЭкспорта[Получатель + ?(НепрямыеРасчетыУПолучателя, "КоррСчетРЦБанка", "КоррСчет")];
	
// СтатусСоставителя, ПлательщикКПП ПолучательКПП ПоказательКБК ОКАТО/ОКТМО ПоказательОснования ПоказательПериода ПоказательНомера ПоказательДаты ПоказательТипа Код
	Если ВыборкаДляЭкспорта.ПеречислениеВБюджет Тогда
		СтрокаЭкспорта.КодБюджетнойКлассификации = ВыборкаДляЭкспорта.КодБК;
	КонецЕсли;
	
// НазначениеПлатежа
	СтрокаЭкспорта.НазначениеПлатежа = СтрЗаменить(ВыборкаДляЭкспорта.НазначениеПлатежа, Символы.ПС + Символы.ВК, " ");
	СтрокаЭкспорта.НазначениеПлатежа = СтрЗаменить(
		СтрЗаменить(СтрЗаменить(СтрокаЭкспорта.НазначениеПлатежа, Символы.ПС, " "), Символы.ВК, " "), Символы.ПФ, " ");
	КоличествоСтрокНП = СтрЧислоСтрок(ВыборкаДляЭкспорта.НазначениеПлатежа);
	
// КодНазначенияПлатежа, ДатаВалютирования, ДатаПолученияТовара
//
	СтрокаЭкспорта.КодНазначенияПлатежа = ВыборкаДляЭкспорта["КодНазначенияПлатежа"];
	СтрокаЭкспорта.ДатаВалютирования    = ВыборкаДляЭкспорта["ДатаВалютирования"];
	СтрокаЭкспорта.ДатаПолученияТовара  = ВыборкаДляЭкспорта["ДатаПолученияТовара"];
	СтрокаЭкспорта.Валюта               = ВыборкаДляЭкспорта["Валюта"].Наименование;

// Руководитель, ГлавныйБухгалтер
	ОтветственныеЛица = ОтветственныеЛицаБППовтИсп.ОтветственныеЛица(ВыборкаДляЭкспорта.Организация, ТекущаяДата());
	Если ЗначениеЗаполнено(ОтветственныеЛица.Руководитель) Тогда
		СтрокаЭкспорта.Руководитель = Строка(ОтветственныеЛица.Руководитель);
	КонецЕсли;
	Если ЗначениеЗаполнено(ОтветственныеЛица.ГлавныйБухгалтер) Тогда
		СтрокаЭкспорта.ГлавныйБухгалтер = Строка(ОтветственныеЛица.ГлавныйБухгалтер);
	КонецЕсли;
	
КонецПроцедуры

Функция СтруктураЭкспорта()
	
	СтруктураЭкспорта = Новый Структура;
	
	СтруктураЭкспорта.Вставить("Выгружать",              ""); // "_"
	СтруктураЭкспорта.Вставить("Готовность",             ""); // "Готовность"
	
	СтруктураЭкспорта.Вставить("НомерДок",               ""); // "№ Документа"
	СтруктураЭкспорта.Вставить("НомерДокумента",         ""); // "Номер"
	СтруктураЭкспорта.Вставить("ДатаДокумента",          ""); // "Дата"
	СтруктураЭкспорта.Вставить("Операция",               ""); // "Операция"
	СтруктураЭкспорта.Вставить("СчетОрганизации",        ""); // "Эл. Р/с организации"
	СтруктураЭкспорта.Вставить("НомерСчетаОрганизации",  ""); // "Р/с организации"
	СтруктураЭкспорта.Вставить("Сумма",                  ""); // "Сумма"
	
	СтруктураЭкспорта.Вставить("ПеречислениеНаКартСчета",  	""); // "Перечисление заработной платы"
	СтруктураЭкспорта.Вставить("ПенсионныеОтчисления", 		""); // "Перечисление пенсионных отчислений"
	СтруктураЭкспорта.Вставить("СоциальныеОтчисления", 		""); // "Перечисление социальных отчислений"
	
	СтруктураЭкспорта.Вставить("Контрагент",             ""); // "Контрагент"
	СтруктураЭкспорта.Вставить("СчетКонтрагента",        ""); // "Р/с контрагента"
	СтруктураЭкспорта.Вставить("НомерСчетаКонтрагента",  ""); // "Эл. Р/с контрагента"
	
