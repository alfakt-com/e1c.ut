	ВыгрузитьПодразделение(ДанныеИБ, ДанныеXDTO);
	
	ДанныеXDTO.КлючевыеСвойства.Вставить("Номер", Лев(ДанныеИБ.Номер, 5) + "В" + Сред(ДанныеИБ.Номер, 7));
	
