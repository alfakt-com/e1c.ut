	ДанныеXDTO.Вставить("КратностьДокумента", ДанныеКурсИКратность.Кратность);
	ВыгрузитьДополнительныеРеквизитыИСведения(КомпонентыОбмена, ДанныеИБ, ДанныеXDTO);
	
	КонтрагентДебитор = ДанныеИБ.КонтрагентДебитор;
	Если ТипЗнч(ДанныеИБ.КонтрагентДебитор) = Тип("СправочникСсылка.Организации") Тогда
		КонтрагентДебитор = КонтрагентИзОрганизации(КонтрагентДебитор, КомпонентыОбмена);
	КонецЕсли;
	
	КонтрагентКредитор = ДанныеИБ.КонтрагентКредитор;
	Если ТипЗнч(ДанныеИБ.КонтрагентКредитор) = Тип("СправочникСсылка.Организации") Тогда
		КонтрагентКредитор = КонтрагентИзОрганизации(КонтрагентКредитор, КомпонентыОбмена);
	КонецЕсли;
	
	ДанныеXDTO.Вставить("КонтрагентДебитор", КонтрагентДебитор);
	ДанныеXDTO.Вставить("КонтрагентКредитор", КонтрагентКредитор);
	
