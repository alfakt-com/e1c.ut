	ДополнительныеДанные = Запрос.ВыполнитьПакет();
	ДанныеШапки = ДополнительныеДанные[3].Выгрузить()[0];
	ДанныеИБ.ДополнительныеСвойства.Вставить("ДанныеШапки", ДанныеШапки);
